import javax.xml.crypto.Data;

public class Banca extends grupo {
	private int id;
	private Data data;
	private String sala;
	
	public Banca(String grupoId, int numero, String nome ,int id, Data data, String sala) {
		super(grupoId, numero, nome);
		this.id = id;
		this.data = data;
		this.sala = sala;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Data getData() {
		return data;
	}

	public void setData(Data data) {
		this.data = data;
	}

	public String getSala() {
		return sala;
	}

	public void setSala(String sala) {
		this.sala = sala;
	}
	
	
	
}
